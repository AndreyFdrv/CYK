#include <stdio.h>
#include <string.h>
void DeleteStartTerminal(char *text)
{
	char terminal=text[0];
	bool IsRightStartTerminal=false;
	for(int i=3; text[i]!='\n', i++)
	{
		if(text[i]==terminal)
			IsRightStartTerminal=true;
	}
	if(!IsRightStartTerminal)
		return;
	char StartTerminal[100]="S0->";
	strcat(StartTerminal, text[0]);
	strcat(StartTerminal, "\n");
	strcat(StartTerminal, text);
	strcpy(text, StartTerminal);
	printf("%s", text);	
}
main(int argc,char *argv[])
{
	FILE* in=fopen(argv[1],"r");
	if(in==NULL)
		printf("File does not exist\n");	
	else
	{
		char text[100];
		fread(text, 100, 1, in);
		DeleteStartTerminal(text);
		fclose(in);
	}
}

